{
  "expo": {
    "name": "GharNirman",
    "slug": "gharnirman",
    "scheme": "aora",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/mainPic.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/mainPic.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E1E2D"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1E1E2D"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
