{
  "expo": {
    "name": "GharNirman",
    "slug": "gharnirman",
    "scheme": "gharnirman",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/mainPic.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/mainPic.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E1E2D"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/mainPic.png",
        "backgroundColor": "#1E1E2D"
      },
      "package": "com.neerajlovecyber.gharnirman"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "05e25c64-1f1c-4ee7-ad0f-efc15f7536e9"
      }
    }
  }
}
